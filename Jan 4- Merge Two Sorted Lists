# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def mergeTwoLists(self, l1: ListNode, l2: ListNode) -> ListNode:
        res=[]
        while l1:
           res.append(l1.val)
           l1=l1.next
        while l2:
           res.append(l2.val)
           l2=l2.val
        res.sort()
        a = dummy = ListNode(0)
        for i in res:
           a.next = ListNode(i)
           a=a.next
        return dummy.next
