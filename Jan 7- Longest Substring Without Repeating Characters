class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        left = 0
        right = 0
        longest = 0
        res= set()
        while right<len(s):
              if s[right] not in res: 
                    res.add(s[right])
                    longest = max(longest,len(res))
                    right+=1
              else:
                    res.remove(s[left])
                    left+=1
        return longest
